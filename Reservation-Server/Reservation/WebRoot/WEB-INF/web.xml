<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <filter>
  	<filter-name>EncodingFilter</filter-name>
  	<filter-class>com.reservation.filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>EncodingFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LevelServlet</servlet-name>
    <servlet-class>com.reservation.servlet.LevelServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TwoLevelServlet</servlet-name>
    <servlet-class>com.reservation.servlet.TwoLevelServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CaiXiServlet</servlet-name>
    <servlet-class>com.reservation.servlet.CaiXiServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>LevelServlet</servlet-name>
    <url-pattern>/LevelServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TwoLevelServlet</servlet-name>
    <url-pattern>/TwoLevelServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CaiXiServlet</servlet-name>
    <url-pattern>/CaiXiServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
